package com.marco6971.AbsolutelyRandomObject.client.settings;

import com.marco6971.AbsolutelyRandomObject.Reference.Names;
import net.minecraft.client.settings.KeyBinding;
import org.lwjgl.input.Keyboard;

public class Keybindings {
	public static KeyBinding charge = new KeyBinding(Names.Keys.CHARGE, Keyboard.KEY_C, Names.Keys.CATEGORY);
	public static KeyBinding release = new KeyBinding(Names.Keys.RELEASE, Keyboard.KEY_R, Names.Keys.CATEGORY);
	public static KeyBinding none = new KeyBinding(Names.Keys.RELEASE, Keyboard.KEY_NONE, Names.Keys.CATEGORY);
}