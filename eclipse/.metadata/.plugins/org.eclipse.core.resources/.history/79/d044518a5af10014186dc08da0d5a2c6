package com.marco6971.AbsolutelyRandomObject.client;

import com.marco6971.AbsolutelyRandomObject.reference.Name;
import net.minecraft.client.settings.KeyBinding;
import org.lwjgl.input.Keyboard;

public class Keybindings
{
    public static KeyBinding charge = new KeyBinding(Name.Keys.CHARGE, Keyboard.KEY_C, Name.Keys.CATEGORY);
    public static KeyBinding release = new KeyBinding(Name.Keys.RELEASE, Keyboard.KEY_R, Name.Keys.CATEGORY);
}